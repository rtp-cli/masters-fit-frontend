{
  "expo": {
    "name": "MastersFit",
    "slug": "mastersfit",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mastersfit.ai",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSHealthShareUsageDescription": "MastersFit needs access to your health data including steps, heart rate, workout sessions, calories burned, and nutrition to provide personalized fitness insights and track your progress.",
        "NSHealthUpdateUsageDescription": "MastersFit may write step count and workout data to HealthKit to help track your fitness progress and maintain accurate health records."
      }
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true
    },
    "android": {
      "splash": {
        "image": "./assets/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mastersfit.ai",
      "permissions": [
        "android.permission.health.READ_STEPS",
        "android.permission.health.WRITE_STEPS",
        "android.permission.health.READ_ACTIVE_CALORIES_BURNED",
        "android.permission.health.READ_HEART_RATE",
        "android.permission.health.READ_EXERCISE_SESSION"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "scheme": "masters-fit",
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-health-connect",
      "expo-notifications",
      [
        "react-native-health",
        {
          "healthSharePermission": "MastersFit reads your health data including steps, heart rate, workouts, calories, and nutrition to provide personalized fitness insights.",
          "healthUpdatePermission": "MastersFit may write step counts and workout data to HealthKit to help track your fitness progress.",
          "isClinicalDataEnabled": false
        }
      ],
      "./plugins/healthconnect",
      "./plugins/healthkit",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "baddaaa3-3d1e-46e5-b15a-f5b8a46ded71"
      }
    },
    "owner": "mastersfit"
  }
}
